<p id="fs-idp144732976">(e)</p>
<p id="fs-idp291225856"><math xmlns="http://www.w3.org/1998/Math/MathML" display="inline"><semantics><mrow><mrow><mtable><mtr><mtd columnalign="left"><mrow><msub><mrow><mtext>NH</mtext></mrow><mn>4</mn></msub><mtext>HS</mtext><mo stretchy="false">(</mo><mi>s</mi><mo stretchy="false">)</mo></mrow></mtd><mtd columnalign="left"><mo stretchy="false">&#8652;</mo></mtd><mtd columnalign="left"><mrow><msub><mrow><mtext>NH</mtext></mrow><mn>3</mn></msub><mo stretchy="false">(</mo><mi>g</mi><mo stretchy="false">)</mo><mo>+</mo></mrow></mtd><mtd columnalign="left"><mrow><msub><mtext>H</mtext><mn>2</mn></msub><mtext>S</mtext><mo stretchy="false">(</mo><mi>g</mi><mo stretchy="false">)</mo></mrow></mtd></mtr><mtr><mtd/><mtd/><mtd columnalign="left"><mi>x</mi></mtd><mtd columnalign="left"><mrow><mtext>___</mtext></mrow></mtd></mtr><mtr><mtd/><mtd/><mtd columnalign="left"><mrow><mn>9.8</mn><mspace width="0.2em"/><mo>&#215;</mo><mspace width="0.2em"/><msup><mrow><mn>10</mn></mrow><mrow><mn>&#8722;6</mn></mrow></msup><mspace width="0.2em"/><mi>M</mi></mrow></mtd><mtd columnalign="left"><mrow><mtext>___</mtext></mrow></mtd></mtr></mtable></mrow></mrow><annotation-xml encoding="MathML-Content"><mrow><mtable><mtr><mtd columnalign="left"><mrow><msub><mrow><mtext>NH</mtext></mrow><mn>4</mn></msub><mtext>HS</mtext><mo stretchy="false">(</mo><mi>s</mi><mo stretchy="false">)</mo></mrow></mtd><mtd columnalign="left"><mo stretchy="false">&#8652;</mo></mtd><mtd columnalign="left"><mrow><msub><mrow><mtext>NH</mtext></mrow><mn>3</mn></msub><mo stretchy="false">(</mo><mi>g</mi><mo stretchy="false">)</mo><mo>+</mo></mrow></mtd><mtd columnalign="left"><mrow><msub><mtext>H</mtext><mn>2</mn></msub><mtext>S</mtext><mo stretchy="false">(</mo><mi>g</mi><mo stretchy="false">)</mo></mrow></mtd></mtr><mtr><mtd/><mtd/><mtd columnalign="left"><mi>x</mi></mtd><mtd columnalign="left"><mrow><mtext>___</mtext></mrow></mtd></mtr><mtr><mtd/><mtd/><mtd columnalign="left"><mrow><mn>9.8</mn><mspace width="0.2em"/><mo>