(e) <math xmlns="http://www.w3.org/1998/Math/MathML" display="inline"><semantics><mrow><mrow><mtable><mtr><mtd><mrow><msub><mrow><mtext>Ca</mtext></mrow><mn>5</mn></msub><mo stretchy="false">(</mo><msub><mrow><mtext>PO</mtext></mrow><mn>4</mn></msub><msub><mrow><mo stretchy="false">)</mo></mrow><mn>3</mn></msub><mtext>OH</mtext><mo stretchy="false">(</mo><mi>s</mi><mo stretchy="false">)</mo><mo stretchy="false">&#8652;</mo></mrow></mtd><mtd><mrow><msup><mrow><mtext>5Ca</mtext></mrow><mtext>2+</mtext></msup><mo stretchy="false">(</mo><mi>a</mi><mi>q</mi><mo stretchy="false">)</mo><mo>+</mo></mrow></mtd><mtd><mrow><msub><mrow><mtext>3PO</mtext></mrow><mn>4</mn></msub><msup><mrow/><mtext>3&#8722;</mtext></msup><mo stretchy="false">(</mo><mi>a</mi><mi>q</mi><mo stretchy="false">)</mo><mo>+</mo></mrow></mtd><mtd><mrow><msup><mrow><mtext>OH</mtext></mrow><mtext>&#8722;</mtext></msup><mo stretchy="false">(</mo><mi>a</mi><mi>q</mi><mo stretchy="false">)</mo></mrow></mtd></mtr><mtr/><mtr><mtd><mrow/></mtd><mtd><mrow><munder accentunder="true"><mrow><mn>5</mn><mi>x</mi></mrow><mo stretchy="true">_</mo></munder></mrow></mtd><mtd><mrow><munder accentunder="true"><mrow><mn>3</mn><mi>x</mi></mrow><mo stretchy="true">_</mo></munder></mrow></mtd><mtd><mi>x</mi></mtd></mtr></mtable></mrow></mrow><annotation-xml encoding="MathML-Content"><mrow><mtable><mtr><mtd><mrow><msub><mrow><mtext>Ca</mtext></mrow><mn>5</mn></msub><mo stretchy="false">(</mo><msub><mrow><mtext>PO</mtext></mrow><mn>4</mn></msub><msub><mrow><mo stretchy="false">)</mo></mrow><mn>3</mn></msub><mtext>OH</mtext><mo stretchy="false">(</mo><mi>s</mi><mo stretchy="false">)</mo><mo stretchy="false">&#8652;</mo></mrow></mtd><mtd><mrow><msup><mrow><mtext>5Ca</mtext></mrow><mtext>2+</mtext></msup><mo stretchy="false">(</mo><mi>a</mi><mi>q</mi><mo stretchy="false">)</mo><mo>+</mo></mrow></mtd><mtd><mrow><msub><mrow><mtext>3PO</mtext></mrow><mn>4</mn></msub><msup><mrow/><mtext>3&#8722;</mtext></msup><mo stretchy="false">(</mo><