<mrow>
<mn>2</mn><mn>2</mn></mrow>
</msub>
<msub>
<mtext>O</mtext>
<mrow>
<mn>1</mn><mn>1</mn></mrow>
</msub>
<mo>=</mo><mn>7.52</mn></mrow>
</mrow><annotation-xml encoding="MathML-Content"><mrow>
<mn>0.0587</mn><mspace width="0.2em"/><mtext>mol</mtext><mspace width="0.2em"/><mtext>KCI</mtext><msub>
<mtext>O</mtext>
<mn>3</mn>
</msub>
<mtext>/</mtext><mn>0.00780</mn><mspace width="0.2em"/><mtext>mol</mtext><mspace width="0.2em"/><msub>
<mtext>C</mtext>
<mrow>
<mn>1</mn><mn>2</mn></mrow>
</msub>
<msub>
<mtext>H</mtext>
